<?xml version="1.0" encoding="utf-8"?>
<ServiceDefinition name="Vestn1.0.Azure" xmlns="http://schemas.microsoft.com/ServiceHosting/2008/10/ServiceDefinition">
  <WebRole name="Vestn1.0" vmsize="ExtraSmall">
    <!--<Sites>
      <Site name="Web">
        <Bindings>
          <Binding name="Endpoint1" endpointName="Endpoint1" />
        </Bindings>
      </Site>
    </Sites>-->
    <Endpoints>
      <InputEndpoint name="Endpoint1" protocol="http" port="80" />
    </Endpoints>
    <Imports>
      <Import moduleName="Diagnostics" />
    </Imports>
    <ConfigurationSettings>
      <Setting name="BlobConnectionString" />
      <Setting name="storageAccountUrl" />
    </ConfigurationSettings>
  </WebRole>
  <WorkerRole name="BackgroundProcesses" vmsize="ExtraSmall">
    <Imports>
      <Import moduleName="Diagnostics" />
    </Imports>
    <ConfigurationSettings>
      <Setting name="BlobConnectionString" />
    </ConfigurationSettings>
  </WorkerRole>
</ServiceDefinition>