@model UserClientMembers.Models.SplashPageModel
           
<!DOCTYPE HTML>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <title>Vestn - Show what you can do.</title>
    <link rel="shortcut icon" href="images/favicon.ico" />
    <script type="text/javascript" src="site.js"></script>
    <script src="http://ajax.googleapis.com/ajax/libs/jquery/1.6.4/jquery.min.js" type="text/javascript"></script>
</head>
<link rel="stylesheet" type="text/css" href="../../Content/Stylesheets/splash.css?1.0.7">
<body>
    <div id="container" class="orangeGradientBackground">
        <div id="header">
            <div id="headerContent">
            </div>
        </div>
        <div id="main">
            <div id="mainContent">
                <div id="privateBetaWindow">
                    <div id="privateBetaTitle">
                        <h3>
                            Private Beta</h3>
                    </div>
                    <div id="privateBetaAccessKeyLabel">
                        <h3>
                            Access Key</h3>
                    </div>
                    @using (Html.BeginForm("AccessBeta", "Home", FormMethod.Post))
                    {
                        
                        @Html.TextBoxFor(m => m.key, new { id = "privateBetaAccessKeyField", @class = "inputBox", placeholder = "Key", tabindex = "1" })

                        <div id="privateBetaSubmitButton">
                            <h3 style="position: absolute; margin-left: 30px; margin-right: 30px; margin-top: 23px;">
                                Get Started
                            </h3>
                        </div>
                    }
                </div>
            </div>
        </div>
        <div class="clearFooter">
        </div>
    </div>
    <!--End Container-->
    <div id="footer" class="orangeGradientBackground">
        <div id="footerBlueTopBorder" class="blueGradientFooter">
        </div>
        <div id="footerContent">
            <h2 style="text-align: center; position: absolute; left: 160px; top: 30px;">
                Stand Out.
            </h2>
        </div>
    </div>
</body>
</html>
